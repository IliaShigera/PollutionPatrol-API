{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "None"
    },
    "AllowedHosts": "*"
  },
  "ConnectionStrings": {
    "UserAccess": "your_connection_string",
    "Pollution": "your_connection_string"
  },
  "External": {
    "DropBox": {
      "AppKey" : "your_app_key",
      "AppSecret" : "your_app_secret",
      "AuthToken": "your_auth_token"
    }
  },
  "Email": {
    "EmailAddress": "your_email_address",
    "Name": "your_name",
    "Password": "your_password",
    "Host": "your_password",
    "Port": "your_port"
  },
  "Authentication": {
    "JWT": {
      "SecretKey": "your_secret_key",
      "Issuer": "your_issuer",
      "Audience": "your_audience",
      "AccessTokenExpirationTimeInMinutes": "your_expiration_time",
      "RefreshTokenExpirationTimeInDays": "your_expiration_time"
    }
  }
}
